{
  "name": "formipeek-qwik",
  "description": "FormiPeek - Free Online File Converter built with Qwik",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.server": "qwik check-client src dist && vite build -c adapters/node-server/vite.config.ts",
    "dev": "vite --mode ssr",
    "preview": "vite preview",
    "serve": "node server/entry.node-server",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.7.0",
    "@builder.io/qwik-city": "^1.7.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "autoprefixer": "^10.4.23",
    "ignore": "^7.0.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.5.3",
    "undici": "^6.19.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
