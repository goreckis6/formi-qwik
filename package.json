{
  "name": "formipeek-qwik",
  "description": "FormiPeek - Free Online File Converter built with Qwik",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build --platform node",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview": "vite preview",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.7.0",
    "@builder.io/qwik-city": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "ignore": "^7.0.5",
    "typescript": "^5.5.3",
    "undici": "^6.19.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
